{
  // Basic
  "extensions.autoUpdate": "onlyEnabledExtensions",
  "security.workspace.trust.untrustedFiles": "open",
  // turn off system sound
  "audioCues.volume": 0,
  "audioCues.diffLineDeleted": "off",
  "audioCues.diffLineInserted": "off",
  "audioCues.diffLineModified": "off",
  "audioCues.lineHasBreakpoint": "off",
  "audioCues.lineHasError": "off",
  "audioCues.lineHasFoldedArea": "off",
  "audioCues.lineHasInlineSuggestion": "off",
  "audioCues.notebookCellCompleted": "off",
  "audioCues.notebookCellFailed": "off",
  "audioCues.onDebugBreak": "off",
  "audioCues.taskCompleted": "off",
  "audioCues.taskFailed": "off",
  "audioCues.terminalCommandFailed": "off",
  "audioCues.terminalQuickFix": "off",
  "files.eol": "\n",
  "files.autoSave": "off",
  "files.autoSaveDelay": 200,
  "files.insertFinalNewline": true,
  "files.simpleDialog.enable": true,
  "files.associations": {
    ".zimrc": "shellscript",
    "*.json": "jsonc"
  },
  "extensions.ignoreRecommendations": true,
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "explorer.openEditors.visible": 20,
  "search.smartCase": true,
  "search.exclude": {
    "**/.git": true,
    "**/.github": true,
    "**/.nuxt": true,
    "**/.output": true,
    "**/.pnpm": true,
    "**/.vscode": true,
    "**/.yarn": true,
    "**/bower_components": true,
    "**/dist/**": true,
    "**/logs": true,
    "**/node_modules": true,
    "**/out/**": true,
    "**/package-lock.json": true,
    "**/pnpm-lock.yaml": true,
    "**/tmp": true,
    "**/yarn.lock": true
  },

  // Window and Workbench
  "window.nativeTabs": false,
  "window.dialogStyle": "custom",
  "window.newWindowDimensions": "offset",
  "window.zoomLevel": 0.4,
  "window.commandCenter": false,
  "window.autoDetectColorScheme": false,
  // sundial
  // "sundial.sunrise": "07:00",
  // "sundial.sunset": "18:30",
  // "sundial.interval": 30,
  // "sundial.daySettings": {},
  // "sundial.nightSettings": {},
  "workbench.sideBar.location": "right",
  "workbench.statusBar.visible": true,
  "workbench.iconTheme": "pop-icon-theme",
  "workbench.productIconTheme": "icons-carbon",
  "workbench.colorTheme": "Panda Syntax",
  // "workbench.preferredLightColorTheme": "Panda Syntax",
  // "workbench.preferredDarkColorTheme": "Panda Syntax",
  "workbench.tree.expandMode": "singleClick",
  "workbench.tree.indent": 10,
  "workbench.list.smoothScrolling": true,
  // 关闭文件后是否聚焦到最近打开过的文件
  "workbench.editor.focusRecentEditorAfterClose": false,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.showTabs": true,
  "workbench.editor.showIcons": true,
  "workbench.editor.closeOnFileDelete": false,
  "workbench.editor.limit.enabled": true,
  "workbench.editor.limit.value": 20,
  "workbench.editor.limit.perEditorGroup": true,

  // Editor
  "emmet.showSuggestionsAsSnippets": true,
  "emmet.useInlineCompletions": true,
  "editor.linkedEditing": true,
  "editor.tabSize": 2,
  "editor.fontSize": 13,
  "editor.lineNumbers": "on",
  "editor.fontFamily": "JetBrains Mono, monospace",
  // "editor.lineHeight": 1.5,
  // "editor.fontFamily": "Monaco, monospace",
  // "editor.fontFamily": "Comic Shanns,  monospace",
  // "editor.fontFamily": "IBM Plex Mono, monospace",
  // "editor.fontFamily": "Input Mono, monospace",
  // "editor.fontFamily": "Fira Code, monospace",
  "editor.fontLigatures": "'ss01', 'ss02', 'ss03', 'ss06', 'zero'",
  "editor.minimap.size": "proportional",
  "editor.suggest.insertMode": "replace",
  "editor.quickSuggestions": {
    "comments": "off",
    "strings": "on"
  },
  "editor.unicodeHighlight.allowedLocales": {
    // 允许中文符号显示而不突出高亮
    "zh-hant": true
  },
  "editor.cursorSurroundingLines": 4,
  "editor.suggest.localityBonus": true,
  "editor.suggest.shareSuggestSelections": true,
  "editor.detectIndentation": false,
  "editor.accessibilitySupport": "off",
  "editor.acceptSuggestionOnEnter": "on",
  "editor.renderLineHighlight": "line",
  "editor.cursorBlinking": "phase",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.find.addExtraSpaceOnTop": false,
  "editor.glyphMargin": true,
  "editor.multiCursorModifier": "alt",
  "editor.renderWhitespace": "selection",
  "editor.unicodeHighlight.invisibleCharacters": false,
  "editor.bracketPairColorization.enabled": true,
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.guides.bracketPairs": "active",
  "editor.smoothScrolling": true,
  "editor.scrollBeyondLastLine": true,
  "editor.stickyScroll.enabled": false,
  "editor.hover.sticky": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": false,
    "source.fixAll.eslint": true
  },
  // NOTE: 从当前主题重写编辑器某些关键字样式
  "editor.semanticTokenColorCustomizations": {
    "enabled": true
  },
  "editor.tokenColorCustomizations": {
    "[Spacegray Eighties Dark]": {
      "comments": "#adb5ad",
      "textMateRules": [
        {
          "scope": [
            // JSON key
            "support.type.property-name"
          ],
          "settings": {
            "foreground": "#ff7675"
          }
        },
        {
          "scope": [
            // 符号：前后引号
            "punctuation.definition.string.begin",
            "punctuation.definition.string.end",
            // 被引号包围的字符
            "string.quoted"
          ],
          "settings": {
            "foreground": "#55efc4"
            // "foreground": "#2bcbba"
          }
        },
        {
          // markdown 无序列表
          "scope": ["markup.list.unnumbered"],
          "settings": {
            "foreground": "#bdc3c7"
          }
        },
        {
          // markdown 链接
          "scope": ["markup.underline"],
          "settings": {
            "foreground": "#48dbfb"
          }
        }
      ]
    },
    "[Vitesse Dark]": {
      "comments": "#909090"
    },
    "[Ayu Mojave]": {
      "comments": "#a2aaa2"
    },
    "[One Dark Pro Darker]": {
      "comments": "#a2aaa2"
    },
    "[Moegi Dark Vitesse]": {
      "comments": "#a1a7a1"
    },
    "[GitHub Dark Dimmed]": {
      "comments": "#adb5ad"
    },
    "[Panda Syntax]": {
      "comments": "#adb5ad",
      "textMateRules": [
        // NOTE: Vue 项目中子组件和组件库标签的颜色关键字
        {
          "scope": ["support.class.component"],
          "settings": {
            "foreground": "#54e4f4",
            "fontStyle": ""
          }
        },
        {
          "scope": [
            "entity.name.tag",
            "entity.name.tag.html",
            "entity.name.tag.css"
            // "meta.tag.structure.any.html",
            // "meta.tag.other.html",
            // "meta.tag.metadata.script.html",
            // "entity.name.tag.jade",
            // "source.vue",
            // "entity.name.tag.pug",
            // "meta.tag.other"
          ],
          "settings": {
            "foreground": "#ed9cc2"
          }
        }
      ]
    },
    "[Moegi Dark]": {
      "comments": "#909090",
      "textMateRules": []
    },
    "textMateRules": [
      {
        "name": "Comment Italics",
        "scope": ["comment", "punctuation.definition.comment"],
        "settings": {
          "fontStyle": ""
        }
      }
      // {
      //   "name": "Keyword Bold",
      //   "scope": ["keyword", "storage.type", "storage.modifier"],
      //   "settings": {
      //     "fontStyle": "bold"
      //   }
      // }
    ]
  },
  "workbench.colorCustomizations": {
    "minimap.background": "#ffffff0a",
    "[Spacegray Eighties Dark]": {
      "tab.border": "#aaaaaa2f",
      "tab.activeBorder": "#86f6deaf"
    },
    "[Panda Syntax]": {
      // "commandCenter.border": "#86f6de8f",
      "badge.background": "#aaaaaa",
      "badge.foreground": "#000000",
      "quickInput.background": "#202020bf",
      "widget.shadow": "#0000008f",
      "titleBar.border": "#aaaaaa4f",
      "tab.border": "#aaaaaa2f",
      "contrastBorder": "#0000008f",
      // "tab.activeBorderTop": "#86f6de",
      // "tab.activeBorder": "#292a2b",
      "tab.activeBorder": "#86f6deaf",
      "editorGroupHeader.tabsBorder": "#aaaaaa4f",
      "textCodeBlock.background": "#aaaaaa4f",
      "editorWidget.foreground": "#cccccc",
      "editorCursor.background": "#ffffff",
      "editor.selectionBackground": "#aaaaaa4f",
      "editor.lineHighlightBackground": "#aaaaaa2f",
      "editor.selectionHighlightBackground": "#aaaaaa2f",
      "editor.wordHighlightBackground": "#aaaaaa2f",
      "editorSuggestWidget.selectedForeground": "#ffffffff",
      "editorSuggestWidget.selectedBackground": "#aaaaaa2f",
      "editorHoverWidget.background": "#202020ef",
      "editorWidget.background": "#202020ef",
      "scrollbarSlider.activeBackground": "#aaaaaa2f",
      "scrollbarSlider.background": "#aaaaaa2f",
