  },

  // Terminal
  "terminal.external.osxExec": "iterm.app",
  "terminal.integrated.gpuAcceleration": "on",
  "terminal.integrated.fontSize": 13,
  "terminal.integrated.fontFamily": "Hack Nerd Font Mono, monospace",
  // "terminal.integrated.fontFamily": "Monaco, Hack Nerd Font Mono, monospace",
  // "terminal.integrated.fontFamily": "Comic Shanns, Hack Nerd Font Mono, monospace",
  // "terminal.integrated.fontFamily": "Input Mono, Hack Nerd Font Mono, monospace",
  // "terminal.integrated.fontFamily": "JetBrains Mono, Hack Nerd Font Mono,monospace",
  // "terminal.integrated.lineHeight": 1.2,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.confirmOnKill": "never",
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1"
  },

  // Vim
  "vim.leader": "<space>",
  "vim.easymotion": false,
  "vim.useSystemClipboard": true,
  "vim.incsearch": true,
  "vim.useCtrlKeys": false,
  "vim.hlsearch": true,
  "vim.surround": true,
  "vim.sneak": true,
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-f>": false,
    "<C-i>": true,
    "<C-o>": true,
    "<C-y>": true
  },
  "vim.insertModeKeyBindings": [],
  "vim.operatorPendingModeKeyBindings": [
    {
      "before": ["H"],
      "after": ["^"]
    },
    {
      "before": ["L"],
      "after": ["g", "_"]
    }
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": ["backspace"],
      "after": ["\"", "_", "c"]
    },
    {
      "before": ["g", "h"],
      "commands": ["editor.action.showHover"]
    },
    {
      "before": ["j"],
      "after": ["g", "j"]
    },
    {
      "before": ["k"],
      "after": ["g", "k"]
    },
    {
      "before": ["enter"],
      "after": ["a", "f"]
    },
    {
      "before": ["p"],
      "after": ["p", "g", "v", "y"]
    },
    {
      "before": ["K"],
      "after": ["4", "k"]
    },
    {
      "before": ["J"],
      "after": ["4", "j"]
    },
    {
      "before": ["H"],
      "after": ["^"]
    },
    {
      "before": ["L"],
      "after": ["g", "_"]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    // Basic
    {
      "before": ["leader", "e"],
      "commands": ["workbench.action.toggleAuxiliaryBar"]
    },
    {
      "before": ["leader", "l"],
      "commands": ["workbench.action.toggleSidebarVisibility"]
    },

    // Editor
    {
      "before": ["n"],
      "after": ["n", "z", "z"]
    },
    {
      "before": ["N"],
      "after": ["N", "z", "z"]
    },
    // DONE: 崔大的 leap 插件的平替，目前使用下来没有问题
    {
      "before": ["s"],
      "commands": ["leap.find"]
    },
    // 覆盖 Vim 提供的原始命令，使用 VSCode 提供的折叠/展开
    {
      "before": ["z", "C"],
      "commands": ["editor.foldAll"]
    },
    {
      "before": ["z", "O"],
      "commands": ["editor.unfoldAll"]
    },
    {
      "before": ["j"],
      "after": ["g", "j"]
    },
    {
      "before": ["k"],
      "after": ["g", "k"]
    },
    {
      "before": ["K"],
      "after": ["5", "g", "k"]
    },
    {
      "before": ["J"],
      "after": ["5", "g", "j"]
    },
    {
      "before": ["H"],
      "after": ["^"]
    },
    {
      "before": ["L"],
      "after": ["g", "_"]
    },
    {
      "before": ["U"],
      "after": ["<c-r>"]
    },
    {
      "before": ["leader", "s"],
      "commands": ["workbench.action.files.save"]
    },
    {
      "before": ["leader", "q"],
      "commands": ["workbench.action.closeActiveEditor"]
    },
    {
      "before": ["leader", "w"],
      "commands": ["workbench.action.files.saveWithoutFormatting"]
    },
    {
      "before": ["leader", ";"],
      "commands": [":noh"]
    },
    {
      "before": ["leader", "d", "f"],
      "commands": ["git.openChange"]
    },
    {
      "before": ["leader", "v", "v"],
      "commands": ["hocusPocus.createVariable"]
    },
    {
      "before": ["leader", "f", "f"],
      "commands": ["hocusPocus.createFunction"]
    },
    {
      "before": ["leader", "["],
      "after": ["[", "{"]
    },
    {
      "before": ["leader", "]"],
      "after": ["]", "}"]
    },

    // Terminal
    {
      "before": ["leader", "t"],
      "commands": ["workbench.action.terminal.focus"]
    }

    // Disable
    // {
    //   "before": ["g", "k"],
    //   "after": ["<c-o>"]
    // },
    // {
    //   "before": ["g", "j"],
    //   "after": ["<c-i>"]
    // },
    // {
    //   "before": ["s", "l", ";"],
    //   "commands": [":vsplit"]
    // },
    // {
    //   "before": ["s", "j", ";"],
    //   "commands": [":split"]
    // },
    // {
    //   "before": ["leader", "a"],
    //   "commands": ["explorer.newFile"]
    // },
    // {
    //   "before": ["leader", "A"],
    //   "commands": ["explorer.newFolder"]
    // },
    // {
    //   "before": ["leader", "k"],
    //   "commands": ["editor.action.marker.prev"]
    // },
    // {
    //   "before": ["leader", "j"],
    //   "commands": ["editor.action.marker.next"]
    // },
    // {
    //   "before": ["Q"],
    //   "commands": ["workbench.action.closeActiveEditor"]
    // },
    // {
    //   "before": ["leader", "f"],
    //   "after": ["g", "f"]
    // }
  ],

  // Comment Anchors
  "commentAnchors.tags.provideAutoCompletion": false,
  "commentAnchors.parseDelay": 300,
  "commentAnchors.workspace.maxFiles": 50,
  "commentAnchors.workspace.excludeFiles": "**/{node_modules,.git,.idea,target,out,build,bin,obj,dist,vendor}/**/*",
  "commentAnchors.tags.separators": [" - ", ": ", " ", "："],
  "commentAnchors.tags.anchors": {
    "ANCHOR": {
      "iconColor": "default",
      "highlightColor": "#A8C023",
      "scope": "file"
    },
    "TODO": {
      "iconColor": "blue",
      "highlightColor": "#3ea8ff",
      "scope": "workspace"
    },
    "BUG": {
      "iconColor": "red",
      "highlightColor": "#F44336",
      "scope": "workspace"
    },
    "FIXME": {
      "iconColor": "red",
      "highlightColor": "#F44336",
      "scope": "workspace"
    },
    "REVIEW": {
      "iconColor": "orange",
      "highlightColor": "#FFB300",
      "scope": "workspace"
    },
    "NOTE": {
      "iconColor": "green",
      "highlightColor": "#64DD17",
      "scope": "file"
    },
    "DONE": {
      "iconColor": "green",
      "highlightColor": "#64DD17",
      "scope": "file"
    },
    "STUB": {
      "scope": "file",
      "enabled": false
    },
    "SECTION": {
      "scope": "file",
      "enabled": false
    },
    "LINK": {
      "scope": "file",
      "enabled": false
    }
  },

  // ErrorLens
  "errorLens.fontFamily": "Monaco, monospace",
  "errorLens.fontWeight": "normal",
  "errorLens.margin": "20px",
  "errorLens.messageTemplate": "$severity($source): $message",
  "errorLens.enabledDiagnosticLevels": ["error", "warning"],
  "errorLens.excludeBySource": ["cSpell", "Grammarly", "eslint"],
  // "errorLens.messageTemplate": "$severity：$message ($source) $count",

  // Formatter
  "editor.formatOnSave": true,
  "prettier.semi": false,
  "prettier.singleQuote": true,
  "prettier.trailingComma": "all",
  "prettier.printWidth": 120,
  "prettier.proseWrap": "never",
  // "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[javascript][jsonc][vue][typescript][html][css][less]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[html]": {
    "editor.suggest.insertMode": "replace"
  },
  "formatFiles.excludedFolders": ["node_modules", ".vscode", ".git", "dist", ".chrome", "attachments"],
  "formatFiles.extensionsToInclude": ".md",

  // Param Hints
  "editor.inlayHints.enabled": "off",
  "javascript.inlayHints.parameterNames.enabled": "none",
  "typescript.inlayHints.parameterNames.enabled": "none",
  "javascript.inlayHints.parameterTypes.enabled": false,
  "typescript.inlayHints.parameterTypes.enabled": false,

  // Git
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "git.untrackedChanges": "separate",
  "git.openRepositoryInParentFolders": "never",
  "js/ts.implicitProjectConfig.checkJs": true,

  // Spell Check
  "cSpell.useGitignore": false,
  "cSpell.enableFiletypes": ["vimrc"],
  "cSpell.userWords": [
    "bilibili",
    "Codegeex",
    "codeium",
    "colour",
    "douyin",
    "easymotion",
    "feng",
    "hlsearch",
    "incsearch",
    "Moegi",
    "peachpuff",
    "powermode",
    "tencent",
    "timi",
    "utools",
    "Vitesse",
    "wechat",
    "weibo",
    "zimfw",
    "zmodule"
  ],

  // Indent Rainbow
  "indentRainbow.indicatorStyle": "light",
  "indentRainbow.lightIndicatorStyleLineWidth": 1,
  "indentRainbow.colors": [
    "rgba(127,255,127,0.3)",
    "rgba(255,255,64,0.3)",
    "rgba(255,127,255,0.3)",
    "rgba(79,236,236,0.3)"
  ],

  // Typing Special Effects
  "powermode.enabled": true,
  "powermode.shake.enabled": false,
  "powermode.explosions.duration": 500,
  "powermode.explosions.maxExplosions": 2,
  "powermode.combo.timerEnabled": "hide",
  "powermode.combo.location": "off",
  "powermode.combo.counterEnabled": "hide",

  // Code Intellisense
  "codeium.enableSearch": false,
  "codeium.enableCodeLens": false,
  "codeium.enableConfig": {
    "*": true,
    "plaintext": true,
    "markdown": true
  },
  "Codegeex.Privacy": false,
  "Codegeex.DisabledFor": {
